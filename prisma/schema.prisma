datasource db {
  provider = "mongodb"
  url      = env("MONGO_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model GuildPrefix {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  guild_id String
  prefix   String @default("lb!")

  @@map("guild_prefixes")
}
